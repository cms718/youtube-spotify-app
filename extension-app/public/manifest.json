{
  "name": "spotify_app",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
      {
          "matches": ["*://*.youtube.com/*"],
          "js": ["content.js"]
      }
  ],
  "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Add to playlist"
  },
  "permissions": ["tabs", "activeTab"],

  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}